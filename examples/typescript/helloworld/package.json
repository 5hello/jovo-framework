{
  "name": "@jovotech/examples-ts-hello-world",
  "version": "4.0.0",
  "private": true,
  "description": "",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "bundle": "ncc build ./src/index.ts",
    "bundle:lambda": "ncc build src/host/lambda.ts && cd dist/ && bestzip ./../bundle.zip index.js",
    "bundle:fastify": "ncc build src/host/fastify.ts",
    "bundle:express": "ncc build src/host/express.ts",
    "bundle:browser": "browserify src/host/browser.ts -p [ tsify --noImplicitAny ] > bundle.js",
    "start": "npm run start:fastify",
    "start:fastify": "cd dist/src/host && node fastify.js",
    "start:express": "cd dist/src/host && node express.js",
    "start:socketio": "cd dist/src/host && node socketio.js",
    "launch": "npm start -- --launch"
  },
  "author": "jovotech",
  "license": "Apache-2.0",
  "dependencies": {
    "@jovotech/framework": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^10.17.50",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@vercel/ncc": "^0.26.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "tsify": "^5.0.2",
    "browserify": "^17.0.0",
    "bestzip": "^2.1.7",
    "fastify": "^3.7.0",
    "express": "^4.17.1",
    "socket.io": "^3.0.0"
  },
  "gitHead": "67f84f1d75587ffa5644a00cf4a4d10e4dfd4a36"
}
