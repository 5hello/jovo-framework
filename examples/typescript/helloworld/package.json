  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "bundle": "ncc build ./src/config.ts",
    "bundle:lambda": "ncc build src/host/lambda.ts -m -s && cd dist/ && bestzip ./../bundle.zip index.js index.js.map",
    "bundle:fastify": "ncc build src/host/fastify.ts",
    "bundle:express": "ncc build src/host/express.ts -m -s",
    "bundle:browser": "browserify src/host/browser.ts -p [ tsify --noImplicitAny ] > bundle.js",
    "start:fastify": "cd dist/host && node fastify.js",
    "start:express": "cd ./dist/host && node express.js",
    "start:socketio": "cd dist/host && node socketio.js",
    "launch": "npm start -- --launch",
    "start": "node ./dist/app.dev.js",
    "start:dev": "tsc-watch --onSuccess \"npm start\"",
    "bundle:dev": "ncc build src/app.dev.ts -s",
    "build": "tsc",
    "bundle:prod": "ncc build src/app.prod.ts -m -s && cd dist/ && bestzip ./../bundle.zip index.js index.js.map"
  },
